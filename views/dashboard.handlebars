{{#if posts.length}}
<section>
    <h2 class="header-page">Your Posts</h2>
    {{#each posts as |post|}}
    {{> dashboard-postinfo post}}
    {{/each}}
</section>
{{/if}}

<button class="btn" type="submit"><a href="/dashboard/new" class="link-button">+ New Post</a></button>
<script src="./js/add-post.js"></script>